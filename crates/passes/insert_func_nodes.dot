digraph {
    0 [ label = "(0) func(n)" ]
    1 [ label = "(1) i = 0" ]
    2 [ label = "(2) if (i < n)" ]
    3 [ label = "(3) i = (i + 1)" ]
    4 [ label = "(4) yield()" ]
    5 [ label = "(5) return()" ]
    6 [ label = "(6) func()" ]
    7 [ label = "(7) func()" ]
    8 [ label = "(8) func()" ]
    0 -> 1 [ label = "" ]
    1 -> 6 [ label = "" ]
    2 -> 8 [ label = "false" ]
    2 -> 7 [ label = "true" ]
    3 -> 4 [ label = "" ]
    4 -> 6 [ label = "" ]
    6 -> 2 [ label = "" ]
    7 -> 3 [ label = "" ]
    8 -> 5 [ label = "" ]
}
